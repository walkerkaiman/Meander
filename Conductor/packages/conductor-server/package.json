    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/ws": "^8.5.5",
    "@types/node": "^20.4.5",
    "@types/cors": "^2.8.13",
    "ts-node-dev": "^2.0.0",
    "tsup": "^7.1.0",
    "typescript": "^5.2.2"
